<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intellize.nb_food_delervery">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.activity.CardActivity" android:screenOrientation="portrait"
            android:parentActivityName=".view.activity.HomeActivity"></activity>
        <activity android:name=".view.activity.DriverActivity" android:screenOrientation="portrait"
            android:parentActivityName=".view.activity.HomeActivity"/>
        <activity
            android:name=".view.activity.OrderRatingActivity"
            android:parentActivityName=".view.activity.MyOrdersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.MyOrdersActivity"
            android:parentActivityName=".view.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.CartActivity"
            android:parentActivityName=".view.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.SigninActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>